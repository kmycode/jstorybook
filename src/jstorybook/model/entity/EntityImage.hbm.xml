<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "classpath://org/hibernate/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="jstorybook.model.entity">
  <class name="jstorybook.model.entity.EntityImage" table="ENTITYIMAGE">
    <id column="ID" name="id" type="java.lang.Long" unsaved-value="null">
      <generator class="increment">
        <!--
                  To add non XDoclet generator parameters, create a file named
                  hibernate-generator-params-Gender.xml
                  containing the additional parameters and place it in your merge dir.
              -->
      </generator>
    </id>
    <property column="path" insert="true" length="512" name="path" type="java.lang.String" update="true"/>
  </class>
</hibernate-mapping>
